{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",

  "ScheduledTasks": {
    "SQLTasks": [
      {
        "ConnectionString": "YourConnectionString",
        "SQLCommandText": "YourSQLCommand",
        "CronExpression": "0 * * * * ?",
        "JobName": "SQLJob1"
      }
    ],
    "ScriptTasks": [
      {
        "ScriptFileName": "YourScriptFilePath",
        "CronExpression": "0 * * * * ?",
        "JobName": "ScriptJob1"
      }
    ]
  }

}
